{
  "kind": "collectionType",
  "collectionName": "moysklad_products",
  "info": {
    "singularName": "moysklad-product",
    "pluralName": "moysklad-products",
    "displayName": "Товары"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "moyskladId": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "code": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::moysklad-category.moysklad-category",
      "inversedBy": "products"
    },
    "price": {
      "type": "integer"
    },
    "priceOld": {
      "type": "integer"
    },
    "uom": {
      "type": "string"
    },
    "weight": {
      "type": "decimal"
    },
    "volume": {
      "type": "decimal"
    },
    "href": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "updated": {
      "type": "datetime"
    },
    "image": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images"
      ]
    },
    "lockImages": {
      "type": "boolean",
      "default": true
    },
    "catalog_collections": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::catalog-collection.catalog-collection",
      "mappedBy": "products"
    },
    "displayTitle": {
      "type": "string"
    }
  }
}
